{
  "name": "sanity-plugin-cincopa-uploader",
  "version": "0.0.6",
  "description": "",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "plugin",
    "video",
    "media",
    "cincopa",
    "cincopa-uploader"
  ],
  "homepage": "https://github.com/Cincopa-com/cincopa-sanity.git#readme",
  "bugs": {
    "url": "https://github.com/Cincopa-com/cincopa-sanity/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Cincopa-com/cincopa-sanity.git"
  },
  "license": "MIT",
  "author": "Davit <davit@cincopa.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "browserslist": "extends @sanity/browserslist-config",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && cp src/index.js src/index.d.ts dist/",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.5"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^7.2.2",
    "@sanity/plugin-kit": "^4.0.19",
    "@types/react": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.10",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sanity": "^3.83.0",
    "styled-components": "^6.1.17",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
  }
